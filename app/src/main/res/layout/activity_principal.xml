<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundologin"
    tools:context=".activity.Administrador.PrincipalActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/bt_resultados"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:onClick="click_resultados"
                android:padding="8dp"
                android:text="Resultados"
                android:textSize="16sp" />

            <Button
                android:id="@+id/bt_kiosque"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:onClick="click_kiosques"
                android:padding="8dp"
                android:text="Terminais de Pesquisa"
                android:textSize="16sp" />

            <Button
                android:id="@+id/bt_questionarios"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:onClick="click_questionarios"
                android:padding="8dp"
                android:text="Questionarios"
                android:textSize="16sp" />

            <Button
                android:id="@+id/bt_principal_deslogar"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:clickable="false"
                android:onClick="click_principal_logar"
                android:padding="8dp"
                android:text="Logar"
                android:textSize="16sp"
                android:visibility="visible" />

            <Button
                android:id="@+id/bt_principal_storage"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:clickable="false"
                android:onClick="click_principal_storage"
                android:padding="8dp"
                android:text="Storage"
                android:textSize="16sp"
                android:visibility="invisible" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>