<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_exibirResultadosResumidos_nomeQuestionario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:hint="Nome do Questionário"
        android:padding="5dp"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/tv_exibirResultadosResumidos_administradorResponsavel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:hint="Administrador Responsável"
        android:padding="5dp"
        android:textColor="@android:color/black" />

</LinearLayout>